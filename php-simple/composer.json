{
    "name": "codecop/lift-pass-pricing",
    "type": "project",
    "require": {
        "php": ">=7.3.0",
        "laravel/lumen-framework": "8.3.*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "9.6.*",
        "mockery/mockery": "1.6.*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "tests": "phpunit"
    },
    "config": {
        "optimize-autoloader": true
    }
}
